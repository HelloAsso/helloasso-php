{
    "name": "helloasso/helloasso-php",
    "version": "1.0.0",
    "description": "HelloAsso API wrapper",
    "homepage": "https://dev.helloasso.com/",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "HelloAsso and contributors",
            "homepage": "https://github.com/HelloAsso/helloasso-php/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
